          <input type="button" class="buttonLeft" value="-"></input>
          <input type="button" class="buttonRight" value="+"></input>
          <div class="cloudText" data-preference = "0">Justice</div>
        </div>

        <div class="cloud col-lg-4">
          <input type="button" class="buttonLeft" value="-"></input>
          <input type="button" class="buttonRight" value="+"></input>
          <div class="cloudText" data-preference = "0">Emploi</div>
        </div>

        <div class="cloud col-lg-4">
          <input type="button" class="buttonLeft" value="-"></input>
          <input type="button" class="buttonRight" value="+"></input>
          <div class="cloudText" data-preference = "0">Environnement</div>
        </div>

      </section>
      <section class="row">
        <input id="submission" type="submit" />
      </section>
    </div>




    <div class="container">
        <% include partials/content %>
        <hr>

        <% include partials/footer %>
    </div>

    <script src="js/selectTheme.js"></script>
    <script src="https://code.jquery.com/jquery.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            $('#home').addClass("active");
        });
    </script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
</body>
</html>
